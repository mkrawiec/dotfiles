[
  {
    "label": "toolbox: enter",
    "tags": ["toolbox"],
    "use_new_terminal": true,
    "reveal": "always",
    "command": "box"
  },
  {
    "label": "httpyac: run current line",
    "tags": ["httpyac"],
    "use_new_terminal": false,
    "reveal": "always",
    "show_command": false,
    "show_summary": false,
    "command": "box -- httpyac --env int --line $ZED_ROW $ZED_FILE"
  },
  {
    "label": "httpyac: edit configuration",
    "tags": ["httpyac"],
    "use_new_terminal": false,
    "reveal": "always",
    "show_command": false,
    "show_summary": false,
    "reveal_target": "center",
    "command": "vi $ZED_WORKTREE_ROOT/httpyac.config.js"
  },
  {
    "label": "tmux: attach",
    "tags": ["tmux"],
    "use_new_terminal": true,
    "reveal": "always",
    "command": "box -- tmux a"
  },
  {
    "label": "tmux: start new session",
    "tags": ["tmux"],
    "use_new_terminal": true,
    "reveal": "always",
    "command": "box -- tmux"
  },
  {
    "label": "git: view log of current file",
    "tags": ["git"],
    "use_new_terminal": false,
    "show_command": false,
    "show_summary": false,
    "reveal": "always",
    "command": "box -- git log --oneline --graph --decorate --color --stat --patch $ZED_FILE"
  }
]
