[user]
    name = Maciek Krawiec
    email = maciek.krawiec@zoho.com
[rerere]
    enabled = 1
    autoupdate = 1
[push]
    default = current
[alias]
    a = add
    ap = add -p
    c = commit
    cm = commit -m
    cam = commit -am
    m = commit --amend --verbose

    d = diff
    ds = diff --staged
    dm = diff master
    dd = diff develop

    s = status -s
    co = checkout
    cob = checkout -b

    f = fetch --prune
    ph = push
    pl = pull
    pr = pull --rebase

    r = rebase
    ro = rebase -s recursive -X ours
    rt = rebase -s recursive -X theirs

    name=rev-parse --abbrev-ref HEAD
    recent=branch --sort=committerdate --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) (%(color:green)%(committerdate:relative)%(color:reset))'
    alias = config --get-regexp ^alias\\.
